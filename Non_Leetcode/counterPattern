class Main {
    public static void main(String[] args) {
       String s = "amazoaan";
       int count = 0;
       char target = 'a'; // You need to define what you are looking for!
       
       for (int i = 0; i < s.length(); i++) {
          char c = s.charAt(i);
       
          if(target == c) {
             count++;
          }
       }
       
       System.out.print(count); // This will output 3
    }
}
